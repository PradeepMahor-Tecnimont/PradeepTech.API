@model IEnumerable<TCMPLApp.Domain.Models.SWPVaccine.VaccinationOfficeRegistrations>
        <table id="registrations" class="table table-striped table-bordered  table-responsive-lg ">
            <thead class="bg-info text-white">
                <tr>
                    <th>Empno</th>
                    <th>Name</th>
                    <th>Parent</th>
                    <th>Grade</th>
                    <th>Emp Type</th>
                    <th>Email</th>
                    <th>COWIN Regtrd</th>
                    <th>Mobile</th>
                    <th>Next JabNumber</th>
                    <th>Transportation</th>
                    <th>BusRoute</th>
                    <th>Attending</th>
                    <th>Remove</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var emp in Model)
                {
                    <tr>

                        <td>@emp.Empno</td>
                        <td>@emp.EmployeeName</td>
                        <td>@(emp.Parent + " - " + emp.DeptDesc)</td>
                        <td>@emp.Grade </td>
                        <td>@emp.Emptype </td>
                        <td>@emp.Email </td>
                        <td>@emp.IsCowinRegistred </td>
                        <td>@emp.MobileNumber </td>
                        <td>@emp.JabNumber </td>
                        <td>@emp.ModeOfTransport </td>
                        <td>@emp.OfficeBusRoute </td>
                        <td>@emp.IsAttendingVaccination </td>


                        <td style="text-align:center">
                            <button class="btn btn-sm btn-outline-danger " id="idRemoveRegistration"
                                    data-url="@Url.Action( "RemoveRegistration","VaccinationOffice")"
                                    data-empno="@emp.Empno"
                                    data-confirmationtext="Remove registration of - @(emp.Empno + " - " + emp.EmployeeName)?">
                                <i class="fa fa-trash" aria-hidden="true"></i>
                            </button>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
