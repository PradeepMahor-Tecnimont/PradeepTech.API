@model IEnumerable<TCMPLApp.Domain.Models.SWPVaccine.SwpEmployeeFamilyRegistrationBatch2>


<table id="idTblFamilyMembers" class="table table-striped table-bordered text-muted text-monospace table-responsive-lg ">
    <thead>
        <tr>
            <th>Family Member Name</th>
            <th>Relation</th>
            <th>Year of Birth</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var m in Model)
            {
                <tr>
                    <td>
                        <span>@m.FamilyMemberName</span>
                    </td>
                    <td>
                        <span>@m.Relation</span>

                    </td>
                    <td>
                        <span>@m.YearOfBirth</span>
                    </td>
                    <td>&nbsp;
                        @*@if (m.Relation.ToUpper() == "SELF")
                        {
                            <span>&nbsp;</span>
                        }
                        else
                        {
                            <button class="btn btn-sm btn-outline-danger " id="idRemoveEntry"
                                    onclick="return deleteFamilyRegistration(this);"
                                    data-url="@Url.Action( "RegisterForBatch2FamilyRemove","VaccinationOffice")"
                                    data-keyid="@m.KeyId"
                                    data-confirmationtext="Remove Vaccine registration for - @(m.FamilyMemberName)?">
                                <i class="fa fa-times" aria-hidden="true"></i>
                            </button>
                        }*@
                    </td>
                </tr>
            }
        }
    </tbody>
</table>
