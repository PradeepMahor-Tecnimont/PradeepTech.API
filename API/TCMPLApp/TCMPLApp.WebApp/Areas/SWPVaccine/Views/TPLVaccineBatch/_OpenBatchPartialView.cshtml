@model IEnumerable<TCMPLApp.Domain.Models.SWPVaccine.SwpVuTplVaccineBatchDet>


<div class="w-100 position-relative ">
   <div class="row" >
        <div class="col-md-6">
                <label>Inoculation Date</label>&nbsp;:&nbsp;<label class="text-info font-weight-bold">@ViewBag.TPLVaccineBatch.VaccineDate.ToString("dd-MMM-yyyy")</label>
        </div>
        <div class="col-md-6 text-right">
            <button name="UploadExcel" id="idUploadExcel" title="Upload Employees in Batch" class="btn btn-outline-primary"
                            data-jqueryselector="openmodal"
                            data-modalcontainer="modalcontainer"
                            data-modalpopupwidth="rightw25"
                            data-url='@Url.Action("ImportBatchEmployees","TPLVaccineBatch")'
                            data-batchid="@ViewBag.TPLVaccineBatch.BatchKeyId"
                            data-statichtmlsourcedivid=""
                            data-modaltitle="Import Batch Employees"
                            data-modalheader="Import Batch Employees"
                    >
                <i class="fas fa fa-upload "></i>&nbsp;&nbsp;Upload Employees to Batch
            </button> &nbsp;&nbsp;&nbsp;
            <button name="ExcelDownload" id="idExcelDownload" title="Excel Download"
                    class=" btn btn-sm btn-success ">
                <i class="fas far fa-file-excel "></i>
            </button>
        </div>
    </div>
    <hr class="m-1" />
</div>
<table id="idDataGrid" class="table table-striped table-bordered  table-responsive-lg " style="width:100%;">
    <thead class="bg-info text-white">
        <tr>
            <th>Empno</th>
            <th>Name</th>
            <th>Parent</th>
            <th>Grade</th>
            <th>Emp Type</th>
            <th>Email</th>
            <th>JabForThisSlot</th>
            <th>TimeSlot</th>
            <th>Transport</th>
            <th>Inoculated</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Empno</td>
                <td>@emp.EmployeeName</td>
                <td>@(emp.Parent + " - " + emp.DepartmentName)</td>
                <td>@emp.Grade </td>
                <td>@emp.Emptype </td>
                <td>@emp.Email </td>
                <td>@emp.JabForThisSlot</td>
                <td>@emp.TimeSlot</td>
                <td>@emp.Transport</td>
                <td>@emp.Inoculated</td>
            </tr>
        }
    </tbody>
</table>
