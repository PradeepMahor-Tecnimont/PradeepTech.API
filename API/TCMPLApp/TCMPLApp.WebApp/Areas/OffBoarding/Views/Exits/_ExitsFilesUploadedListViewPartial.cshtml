@model IEnumerable<TCMPLApp.Domain.Models.OffBoarding.OffBoardingFilesUploaded>

<div class="  bg-gray-active border rounded ">
    <table id="tbOffBoardingExits" class="table table-striped table-bordered table-responsive-lg ">
        <thead class="bg-info text-white">
            <tr>
                <th>FileName</th>
                <th>Upload by</th>
                <th>Upload Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var m in Model)
            {
                <tr>
                    <td>
                        <a href="#" onclick="ofbfileDownload(event, this);"
                            data-url='@Url.Action("DownloadFile", "Exits", new { Area = "OffBoarding" })'
                            data-keyid="@m.KeyId"
                            data-clientfilename="@m.ClientFileName"
                            > @m.ClientFileName</a></td>

                    <td>@(m.UploadByEmpno + " - " + m.UploadByEmployeeName)</td>
                    <td>@m.UploadDate.ToString("dd-MMM-yyyyy HH:mm")</td>
                </tr>
            }
        </tbody>
    </table>
    <div id="result"></div>
</div>
